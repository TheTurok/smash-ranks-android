<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:background="@color/gray"
    android:layout_height="wrap_content"
    android:layout_width="match_parent"
    android:paddingBottom="@dimen/root_padding_half"
    android:paddingLeft="@dimen/root_padding"
    android:paddingRight="@dimen/root_padding"
    android:paddingTop="@dimen/root_padding_half"
    android:orientation="vertical"
    tools:ignore="Overdraw">
    <TextView
        android:id="@+id/model_log_message_with_stack_trace_tag_and_message"
        android:layout_height="wrap_content"
        android:layout_marginBottom="@dimen/root_padding_fourth"
        android:layout_width="match_parent"
        android:textSize="14sp"
        tools:text="Region listener added at index 4" />
    <TextView
        android:id="@+id/model_log_message_with_stack_trace_stack_trace"
        android:layout_height="wrap_content"
        android:layout_width="match_parent"
        android:textSize="11sp"
        tools:ignore="SmallSp"
        tools:text="Exception at line 54 in Object.toString()" />
</LinearLayout>
