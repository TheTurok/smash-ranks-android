<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="drawer_layout" parent="root.no_padding">
        <item name="android:fitsSystemWindows">true</item>
        <item name="android:id">@id/drawer_layout</item>
    </style>

    <style name="fontFamily_medium">
        <item name="android:fontFamily">sans-serif-medium</item>
    </style>

    <style name="navigation_drawer">
        <item name="android:fitsSystemWindows">true</item>
        <item name="android:layout_gravity">start</item>
    </style>

    <style name="separator_text" parent="separator_text_base">
        <item name="android:fontFamily">sans-serif-medium</item>
    </style>

    <style name="settings_separator" parent="settings_separator_base">
        <item name="android:fontFamily">sans-serif-medium</item>
    </style>

    <style name="status_bar_buffer" parent="status_bar_buffer_base">
        <item name="android:visibility">visible</item>
    </style>

</resources>
